<h4>Event Details</h4>
<br />
<div id="appEvent" class="col-md-4">
    <div class="form-group">
        <label for="activity" class="control-label">Activity: </label>
        <input id="activity" v-model="eventData.activity" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label for="responsable">Organizer: </label>
        <input id="organizer" v-model="eventData.organizer" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label for="city" >City: </label>
        <input id="city" v-model="eventData.city" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label for="street" class="control-label">Street: </label>
        <input id="street" v-model="eventData.street" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label for="number" class="control-label">Number: </label>
        <input id="number" v-model="eventData.number" readonly class="form-control" />
    </div>
    <div class="form-group">
        <label for="reference" class="control-label">Reference: </label>
        <input id="reference" v-model="eventData.reference" readonly class="form-control" />
    </div>
</div>
<div class="form-group">
    <a asp-action="Index">Back to the List</a>
</div>
<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="https://unpkg.com/vue@3.0.3/dist/vue.global.js"></script>
<script>
    const appEvent = Vue.createApp({
        data() {
            return {
                eventData: @Html.Raw(ViewBag.Data)
            }
        }
    });
    appEvent.mount('#appEvent');
</script>