@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Multiply</title>
</head>
<body>
    @for (int i = 1; i <= 10; i++)
    { 
        <h4>
            @i x @ViewBag.Multiplier = @(i * ViewBag.Multiplier)
        </h4>
    }
</body>
</html>
